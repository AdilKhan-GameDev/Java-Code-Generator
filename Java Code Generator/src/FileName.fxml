<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="750.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FileNameController">
    <stylesheets>
        <URL value="@filename.css" />
    </stylesheets>
   <children>
      <VBox layoutX="307.0" layoutY="56.0" />
      <Label alignment="CENTER" layoutX="279.0" layoutY="56.0" text="Provide File Name!!!" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="232.0" layoutY="86.0" text="Your File Name will be your Major/Parent Class Name." />
      <TextField fx:id="filename" layoutX="301.0" layoutY="161.0" promptText="File/Class Name" />
      <Button fx:id="next" alignment="CENTER" contentDisplay="CENTER" layoutX="334.0" layoutY="284.0" mnemonicParsing="false" onAction="#NextButton" prefHeight="33.0" prefWidth="82.0" text="Next" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
